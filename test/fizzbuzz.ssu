import console;

func void
fizzbuzzrange(int start, int end, int step)
{
	for (var int i = start; i < end; i += step)
	{
		fizzbuzz(i);
	}
}

func void
fizzbuzz(int n)
{
	var string accum = "";
	
	if (n % 3 == 0)
	{
		accum += "fizz";
	}
	if (n % 5 == 0)
	{
		accum += "buzz";
	}
	
	if (len(accum) == 0)
	{
		console.println("${n}");
	}
	else
	{
		console.println(accum);
	}
}

func int
start()
{
	fizzbuzzrange(0, 100, 1);
	return 0;
}
