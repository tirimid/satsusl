import std_console;

func void
fizzbuzzrange(int lb, int ub, int step)
{
	for (new int i = lb; i < ub; i += step)
	{
		fizzbuzz(i);
	}
}

func void
fizzbuzz(int n)
{
	new string accum = "";
	
	if (n % 3 == 0)
	{
		accum += "fizz";
	}
	if (n % 5 == 0)
	{
		accum += "buzz";
	}
	
	if (accum == "")
	{
		std_println("{n}");
	}
	else
	{
		std_println(accum);
	}
}

func void
start()
{
	fizzbuzzrange(0, 100, 1);
}
