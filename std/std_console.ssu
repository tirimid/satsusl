func void
std_print(string msg)
{
	system void cprint(msg);
}

func void
std_println(string msg)
{
	system void cprint(msg + "\n");
}

func string
std_readln()
{
	return system string creadln();
}

func string
std_readprompt(string msg)
{
	system void cprint(msg);
	return system string creadln();
}
